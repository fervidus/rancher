{
  "puppet_task_version": 1,
  "supports_noop": false,
  "description": "Set Rancher administrator password",
  "files": ["ruby_task_helper/files/task_helper.rb"],
  "input_method": "stdin",
  "parameters": {
    "login_token": {
      "type": "String[1]",
      "description": "Login token."
    },
    "current_password": {
      "type": "String[1]",
      "description": "Current admin password."
    },
    "new_password": {
      "type": "String[1]",
      "description": "New admin password."
    },
    "rancher_server": {
      "type": "String[1]",
      "description": "FQDN of Rancher server."
    }
  }
}
