{
  "puppet_task_version": 1,
  "supports_noop": false,
  "description": "URL by which cluster controller(s)/worker(s) refer to Rancher. Must be HTTPS.",
  "files": ["ruby_task_helper/files/task_helper.rb"],
  "input_method": "stdin",
  "parameters": {
    "api_token": {
      "type": "String[1]",
      "description": "The login token password."
    },
    "rancher_url": {
      "type": "String[1]",
      "description": "Rancher URL."
    },
    "rancher_server": {
      "type": "String[1]",
      "description": "FQDN of Rancher server."
    }
  }
}
